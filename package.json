{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for expressjs framework in typescript language",
  "main": "build/src/server.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "shx rm -rf build",
    "start": "node build/src/server.js",
    "watch": "tsc-watch --onSuccess \"npm run start\"",
    "seed": "node build/database/seed/index.js",
    "test": "mocha build/test --recursive --reporter mochawesome --reporter-options reportDir=storage/test_report/ --exit"
  },
  "author": "Vignesh Chidambaram<vignesh.jag@gmail.com>",
